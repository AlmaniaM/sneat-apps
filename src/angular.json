{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "pnpm",
    "analytics": "648b39e6-de60-4652-879b-3b251331fa16"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "emptyapp",
  "projects": {
    "emptyapp": {
      "projectType": "application",
      "root": "apps/emptyapp",
      "sourceRoot": "apps/emptyapp/src",
      "prefix": "sneat",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/emptyapp",
            "index": "apps/emptyapp/src/index.html",
            "main": "apps/emptyapp/src/main.ts",
            "polyfills": "apps/emptyapp/src/polyfills.ts",
            "tsConfig": "apps/emptyapp/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/emptyapp/src/favicon.ico",
              "apps/emptyapp/src/assets"
            ],
            "styles": ["apps/emptyapp/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/emptyapp/src/environments/environment.ts",
                  "with": "apps/emptyapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "emptyapp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "emptyapp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "emptyapp:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/emptyapp/src/**/*.ts",
              "apps/emptyapp/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/emptyapp"],
          "options": {
            "jestConfig": "apps/emptyapp/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "emptyapp-e2e": {
      "root": "apps/emptyapp-e2e",
      "sourceRoot": "apps/emptyapp-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/emptyapp-e2e/cypress.json",
            "tsConfig": "apps/emptyapp-e2e/tsconfig.e2e.json",
            "devServerTarget": "emptyapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "emptyapp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/emptyapp-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
